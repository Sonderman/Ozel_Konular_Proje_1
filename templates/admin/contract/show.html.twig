{% extends 'base.html.twig' %}

{% block title %}Contract{% endblock %}

{% block body %}
    <h1>Contract</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contract.id }}</td>
            </tr>
            <tr>
                <th>Customer_id</th>
                <td>{{ contract.customerId }}</td>
            </tr>
            <tr>
                <th>Pickup_date</th>
                <td>{{ contract.pickupDate ? contract.pickupDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Dropoff_date</th>
                <td>{{ contract.dropoffDate ? contract.dropoffDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Pickup_location</th>
                <td>{{ contract.pickupLocation }}</td>
            </tr>
            <tr>
                <th>Dropoff_location</th>
                <td>{{ contract.dropoffLocation }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_contract_index') }}">back to list</a>

    <a href="{{ path('admin_contract_edit', {'id': contract.id}) }}">edit</a>

    {{ include('admin/contract/_delete_form.html.twig') }}
{% endblock %}
